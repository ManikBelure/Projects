package sdbms;

public class Student {
	
     //4 private Data members->id,age,name,marks
	//constructor
	//3 setter and 4 getter methods
	//toString()->id=100, age=22, Name=Manik, Marks=80
	
	private String id;
	private int age;
	private String name;
	private int marks;
	public Student(String id, int age, String name, int marks) {
		super();
		this.id = id;
		this.age = age;
		this.name = name;
		this.marks = marks;
	}
	
	
}
