package lambdaexperssions;

@FunctionalInterface
interface Person
{
	String display(int age);
}

class Tom implements Person{

	@Override
	public String display(int age) {
		return "age "+age;
	}
	
}

public class Test {

	public static void main(String[] args) {
		//subclass
		Tom t=new Tom();
		System.out.println(t.display(23));
	
	}

}
